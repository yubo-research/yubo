
FILES=`ls */*.py`

echo Temporary code:
grep -e TEST -e HACK $FILES

echo
echo Missing files:
git status $FILES */*.html */*/*.html */*.jinja */*/*.jinja | grep -e py$ -e html$ -e jinja$ | grep -v :

echo
echo Formatting:
black --line-length=160 -q $FILES

echo
echo Lint:
ruff --config config/ruff_config --exclude third_party/ --force-exclude --fix --fixable I001 *



